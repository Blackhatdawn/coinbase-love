# Development Environment Configuration
# These settings are used when running `npm run dev`

# API Base URL - Uses Vite proxy to forward to local backend
# Leave empty to use relative paths with Vite proxy
VITE_API_BASE_URL=

# Alternative: Use direct localhost connection (bypass proxy)
# VITE_API_BASE_URL=http://localhost:8001

# App Configuration
VITE_APP_NAME=CryptoVault
VITE_APP_VERSION=1.0.0
VITE_NODE_ENV=development

# Sentry (optional in development)
VITE_SENTRY_DSN=
