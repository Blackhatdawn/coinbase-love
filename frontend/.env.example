# Frontend Environment Variables
# Copy this file to .env.local and fill in values for local development

# Backend API URL (Required for production)
# Development: Leave empty to use Vite proxy (localhost:8001)
# Production (Vercel): Must be set to your Render backend URL
# Example: https://coinbase-love.onrender.com
VITE_API_URL=

# Optional: Enable debug logging (development only)
VITE_DEBUG=false

# Note: VITE_ prefix is required for Vite to expose env vars to the browser
# See: https://vitejs.dev/guide/env-and-mode.html
